.ws-black { background-color: rgb(40, 42, 53) !important; color: white !important; }

.ws-hover-black:hover { background-color: rgb(0, 0, 0) !important; color: white !important; }

.ws-grey { background-color: rgb(231, 233, 235) !important; color: black !important; }

.ws-yellow { background-color: rgb(255, 244, 163) !important; color: black !important; }

.ws-green { background-color: rgb(4, 170, 109) !important; color: white !important; }

.ws-text-green { color: rgb(4, 170, 109) !important; }

.ws-hover-green:hover { background-color: rgb(5, 152, 98) !important; color: white !important; }

.ws-hover-text-green:hover { color: rgb(5, 152, 98) !important; }

.ws-light-green { background-color: rgb(217, 238, 225) !important; color: black !important; }

.ws-turquoise { background-color: rgb(150, 212, 212) !important; color: black !important; }

.ws-pink { background-color: rgb(255, 192, 199) !important; color: black !important; }

.ws-light-pink { background-color: rgb(243, 236, 234) !important; color: black !important; }

.ws-btn { border: none; border-radius: 5px; display: inline-block; padding: 8px 16px; vertical-align: middle; overflow: hidden; text-decoration: none; text-align: center; cursor: pointer; white-space: nowrap; user-select: none; background-color: rgb(4, 170, 109); color: rgb(255, 255, 255); }

.ws-btn:hover, .ws-btn:active { color: rgb(255, 255, 255); background-color: rgb(5, 152, 98) !important; }

a.btnsmall:hover { box-shadow: none; }

a.btnsmall:active, a.btnsmall:hover { color: rgb(255, 255, 255); }

a.btnplayit:hover, a.btnplayit:active { color: rgb(255, 255, 255); background-color: rgb(255, 153, 0) !important; }

a.btnplayit:hover { box-shadow: none; }

#w3-exerciseform { padding: 20px; margin: 32px -20px; }

p { margin-top: 1.2em; margin-bottom: 1.2em; font-size: 15px; }

hr { margin: 20px -16px; }

.w3-codespan { font-size: 105%; background-color: rgba(222, 222, 222, 0.3); }

.w3-example p, .w3-info p, .w3-note p, .w3-warning p { margin-top: 1em; margin-bottom: 1em; }

.w3-code, w3-codeline { font-size: 15px; }

#midcontentadcontainer, #mainLeaderboard { text-align: center; margin-left: -16px; margin-right: -16px; }

.adtext { font-family: "Segoe UI", Arial, sans-serif; font-size: 12px; color: rgb(119, 119, 119); margin-top: 0px; margin-bottom: 0px; padding-top: 0px; padding-bottom: 0px; }

#snhb-mid_content-0 { height: 292px; width: 100%; position: relative; display: table; }

#snhb-mid_content-0 > p { top: 0px; left: 0px; width: 100%; position: absolute; display: block; }

#snhb-mid_content-0 > div { padding-top: 15px; vertical-align: middle; text-align: center; width: 100% !important; display: table-cell !important; }

#snhb-wide_skyscraper-0 { height: 612px; position: relative; display: block; }

@media screen and (max-width: 974px) {
  #snhb-wide_skyscraper-0 { height: 62px; }
}

@media screen and (max-width: 600px) {
  .w3-example, #w3-exerciseform { margin-left: -32px; margin-right: -32px; border-radius: 0px; }
}

@media only screen and (max-width: 500px) {
  #main { padding: 16px; }
  h1 { font-size: 30px; }
  h2 { font-size: 25px; }
  .w3-example { padding: 8px 16px; margin: 24px -16px; }
  #w3-exerciseform { padding: 8px 16px 16px; margin: 24px -16px; }
  .w3-note, .w3-info, .w3-warning { margin-left: -16px; margin-right: -16px; padding-left: 8px !important; padding-right: 8px !important; }
}
